<h1>Create New Playlist</h1>
<%= render 'errors' %>
<%= bootstrap_form_for(@playlist) do |f| %>
<%= f.text_field :title, label_as_placeholder: true %>
<br>
<%= f.text_area :story, label_as_placeholder: true %>
<br>
<%= f.text_field :image_url, placeholder: 'Add image url', hide_label: true %>
<small><p class="text-muted">copy and past image address</p></small>
<br>
<%= f.submit "Submit" %>
<% end %>
<%= link_to 'Back', url_for(:back) %>
