<h1>Log In</h1>
<div class="form">
<%= bootstrap_form_with url: sessions_path  do |f| %>
<%= f.text_field :email, placeholder: "Email", hide_label: true, required: true %>
<br>
<%= f.password_field :password, placeholder: "Password", hide_label: true, required: true %>
<br>
<%= f.submit "Log In" %>
<% end %>
<%= link_to 'Back', url_for(:back) %>
</div>
